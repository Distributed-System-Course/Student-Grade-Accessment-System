<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.studentaccessment.mapper.GroupMapper">
<select id = "getInfo" parameterType="int" resultType="com.example.studentaccessment.bean.GroupBean">
    SELECT * FROM information WHERE groupID =#{groupID}
</select>
</mapper>
