<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.crud.mapper.CommitMapper">

    <!--查询所有用户-->
    <select id="clist" resultType="Commit">
        select * from commit
    </select>

    <select id="findCommitByProject" resultType="Commit">
        select * from commit where project = #{project}
    </select>
</mapper>